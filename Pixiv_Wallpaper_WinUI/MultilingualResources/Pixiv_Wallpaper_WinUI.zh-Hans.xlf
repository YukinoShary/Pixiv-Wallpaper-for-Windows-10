          <source>Fail to get following users' updating queue</source>
          <target state="translated">获取已关注用户的插画更新队列失败</target>
        </trans-unit>
        <trans-unit id="FollowingUserUpdating.Content" translate="yes" xml:space="preserve">
          <source>Following users' updating</source>
          <target state="translated">已关注用户的插画更新</target>
