          <source>The app will store your account and password locally</source>
          <target state="translated">应用会储存你的账号与密码</target>
        </trans-unit>
        <trans-unit id="Recommend.Content" translate="yes" xml:space="preserve">
          <source>Pixiv Recommendation </source>
          <target state="translated">Pixiv 推荐 </target>
        </trans-unit>
