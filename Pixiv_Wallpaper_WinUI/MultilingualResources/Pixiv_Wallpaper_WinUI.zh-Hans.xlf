          <target state="translated">深色</target>
        </trans-unit>
        <trans-unit id="Light" translate="yes" xml:space="preserve">
          <source>Light</source>
          <target state="translated">浅色</target>
        </trans-unit>
        <trans-unit id="ThemeSelect.Text" translate="yes" xml:space="preserve">
          <source>Color Theme</source>
          <target state="needs-review-translation">颜色主题</target>
          <note from="MultilingualUpdate" annotates="source" priority="2">Please verify the translation’s accuracy as the source string was updated after it was translated.</note>
        </trans-unit>
        <trans-unit id="ThemeSelectExplain.Text" translate="yes" xml:space="preserve">
          <source>This option will be applied after the app restarts</source>
          <target state="translated">此项更改将在应用下次启动时生效</target>
        </trans-unit>
        <trans-unit id="FailToLoginTry" translate="yes" xml:space="preserve">
          <source>Fail to login with the "no SNI mode", now try to login one more time with the normal mode. It's required to re-enter your account and password.</source>
          <target state="new">Fail to login with the "no SNI mode", now try to login one more time with the normal mode. It's required to re-enter your account and password.</target>
        </trans-unit>
